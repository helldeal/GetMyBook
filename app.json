{
  "expo": {
    "name": "GetMyBook",
    "slug": "getmybook",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#b70707"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.helldeal.getmybook"
    },
